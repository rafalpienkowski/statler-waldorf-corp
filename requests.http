# TEAMS controller

# Get all teams
GET http://localhost:8080/api/teams HTTP/1.1

###

# Add team
POST http://localhost:8080/api/teams HTTP/1.1
Content-Type: application/json

{
    "Name": "First team",
    "Id": "9c7489c4-5d6c-4859-9526-98463811b49d"
}

###

# Delete team
DELETE http://localhost:8080/api/teams/9c7489c4-5d6c-4859-9526-98463811b49d HTTP/1.1

###

# Update team
PUT http://localhost:8080/api/teams/9c7489c4-5d6c-4859-9526-98463811b49d HTTP/1.1
Content-Type: application/json

{
    "Name": "First team - updated",
    "Id": "9c7489c4-5d6c-4859-9526-98463811b49d"
}

###

# Get team by id
GET http://localhost:8080/api/teams/9c7489c4-5d6c-4859-9526-98463811b49d HTTP/1.1

###

# MEMBERS controller

# Get all members for a team
GET http://localhost:8080/api/teams/9c7489c4-5d6c-4859-9526-98463811b49d/members HTTP/1.1

###

# Add team member
POST http://localhost:8080/api/teams/9c7489c4-5d6c-4859-9526-98463811b49d/members HTTP/1.1
Content-Type: application/json

{
    "FirstName": "John",
    "LastName": "Smith",
    "Id": "5152cfb5-c44e-4b10-9e89-0b2dc61d8ba3"
}

###

# Get team member
GET http://localhost:8080/api/teams/9c7489c4-5d6c-4859-9526-98463811b49d/members/5152cfb5-c44e-4b10-9e89-0b2dc61d8ba3 HTTP/1.1

###

# Update a team member
PUT http://localhost:8080/api/teams/9c7489c4-5d6c-4859-9526-98463811b49d/members/5152cfb5-c44e-4b10-9e89-0b2dc61d8ba3 HTTP/1.1
Content-Type: application/json

{
    "FirstName": "John W.",
    "LastName": "Smith",
    "Id": "5152cfb5-c44e-4b10-9e89-0b2dc61d8ba3"
}

###

# Delete a team member
DELETE http://localhost:8080/api/teams/9c7489c4-5d6c-4859-9526-98463811b49d/members/5152cfb5-c44e-4b10-9e89-0b2dc61d8ba3 HTTP/1.1
